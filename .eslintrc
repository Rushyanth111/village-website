{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-redux/recommended"
    ],
    "plugins": [
        "react-redux"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "env": {
        "node":true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "react-redux/mapDispatchToProps-prefer-shorthand":"off",
        "react-redux/prefer-separate-component-file":"off"
    }
}